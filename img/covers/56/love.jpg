								  'isVisible':false,
								  'element':document.getElementById(id),
								  'dropdown':null,
								  'highlighted':null};
	}
	,hideDropdown:function(id)
	{
		 if (midtownSearchSugg.__AutoComplete[id]['iframe']) {
            midtownSearchSugg.__AutoComplete[id]['iframe'].style.visibility = 'hidden';
        }


        midtownSearchSugg.__AutoComplete[id]['dropdown'].style.visibility = 'hidden';
        midtownSearchSugg.__AutoComplete[id]['highlighted'] = null;
        midtownSearchSugg.__AutoComplete[id]['isVisible']   = false;
	}
	,createDropdown:function(id)
	{
			 
		this.__AutoComplete[id]['dropdown'] = document.createElement('div');
        this.__AutoComplete[id]['dropdown'].className = 'autocomplete'; // Don't use setAttribute()
    
        this.__AutoComplete[id]['element'].parentNode.insertBefore(this.__AutoComplete[id]['dropdown'], this.__AutoComplete[id]['element']);
        // Position it
        this.__AutoComplete[id]['dropdown'].style.left       = '130px';
