		this.__AutoComplete[id]['dropdown'].style.visibility = 'hidden';
		 if (this.isIE)
		 {
		 	this.__AutoComplete[id]['iframe'] = document.createElement('iframe');
            this.__AutoComplete[id]['iframe'].id = id +'_iframe';
            this.__AutoComplete[id]['iframe'].style.position = 'absolute';
            this.__AutoComplete[id]['iframe'].style.top = '0';
            this.__AutoComplete[id]['iframe'].style.left = '0';
            this.__AutoComplete[id]['iframe'].style.width = '0px';
            this.__AutoComplete[id]['iframe'].style.height = '0px';
            this.__AutoComplete[id]['iframe'].style.zIndex = '6000';
            this.__AutoComplete[id]['iframe'].style.visibility = 'hidden';
            this.__AutoComplete[id]['element'].parentNode.insertBefore(this.__AutoComplete[id]['iframe'],this.__AutoComplete[id]['element']);
				 
		 }
		 
		 // Hides the dropdowns when document clicked
        var docClick = function()
        {
			//alert(midtownSearchSugg.__AutoComplete)
           for (id in midtownSearchSugg.__AutoComplete) {
		   		 midtownSearchSugg.hideDropdown(id);
           }
        }
		// alert(docClick)
		 if (document.addEventListener) {
		    document.addEventListener('click', docClick, false);
        } else if (document.attachEvent) {
			document.attachEvent('onclick', docClick, false);
        }
	
	}
	,setEvents:function(id)
	{
		this.__AutoComplete[id]['element'].setAttribute('autocomplete', 'off');
		this.__AutoComplete[id]['element'].onkeydown  = function(e) {return midtownSearchSugg.AutoComplete_KeyDown(this.getAttribute('id'), e);}
        this.__AutoComplete[id]['element'].onkeyup    = function(e) {return midtownSearchSugg.AutoComplete_KeyUp(this.getAttribute('id'), e);}
        this.__AutoComplete[id]['element'].onkeypress = function(e) {if (!e) e = window.event; if (e.keyCode == 13 /*|| midtownSearchSugg.isOpera*/) return false;}
       // this.__AutoComplete[id]['element'].ondblclick = function() {midtownSearchSugg.AutoComplete_ShowDropdown(this.getAttribute('id'));}
        this.__AutoComplete[id]['element'].onclick    = function(e) {if (this.value=="Search by title, character, or creator")this.value="";if (!e) e = window.event; e.cancelBubble = true; e.returnValue = false;}
	}
	,AutoComplete_KeyDown:function(id)
	{
		
		     // Mozilla
        if (arguments[1] != null) {
            event = arguments[1];
        }

        var keyCode = event.keyCode;
		
		//document.getElementById("error_display").innerHTML=keyCode
		
//alert(keyCode)
        switch (keyCode) {

            // Return/Enter
            case 13:
                if (midtownSearchSugg.__AutoComplete[id]['highlighted'] != null) {
                    midtownSearchSugg.AutoComplete_SetValue(id);
                    midtownSearchSugg.AutoComplete_HideDropdown(id);
					document.getElementById("frmsearchtop").submit();
                }
				else
				{
					document.getElementById("frmsearchtop").submit();
				}
                
                event.returnValue = false;
                event.cancelBubble = true;
                break;

            // Escape
            case 27:
                midtownSearchSugg.AutoComplete_HideDropdown(id);
                event.returnValue = false;
                event.cancelBubble = true;
                break;
            
            // Up arrow
            case 38:
                if (!midtownSearchSugg.__AutoComplete[id]['isVisible']) {
                    //midtownSearchSugg.AutoComplete_ShowDropdown(id);
                }
                
                midtownSearchSugg.AutoComplete_Highlight(id, -1);
                midtownSearchSugg.AutoComplete_ScrollCheck(id, -1);
                return false;
                break;
            
            // Tab
            case 9:
                if (midtownSearchSugg.__AutoComplete[id]['isVisible']) {
                    midtownSearchSugg.AutoComplete_HideDropdown(id);
                }
                return;
            
            // Down arrow
            case 40:
                if (!midtownSearchSugg.__AutoComplete[id]['isVisible']) {
                   // midtownSearchSugg.AutoComplete_ShowDropdown(id);
                }
                
                midtownSearchSugg.AutoComplete_Highlight(id, 1);
                midtownSearchSugg.AutoComplete_ScrollCheck(id, 1);
                return false;
                break;
        }
	}
	,AutoComplete_KeyUp:function(id)
	{
		// Mozilla
        if (arguments[1] != null) {
            event = arguments[1];
        }
		var keyCode = event.keyCode;
		
        switch (keyCode) {
            case 13:
                event.returnValue = false;
                event.cancelBubble = true;
                break;

            case 27:
                midtownSearchSugg.AutoComplete_HideDropdown(id);
                event.returnValue = false;
                event.cancelBubble = true;
                break;
            
            case 38:
            case 40:
                return false;
                break;
			default:
				midtownSearchSugg.AutoComplete_ShowDropdown(id);
                break;
        }
	
	}
	,AutoComplete_ShowDropdown : function(id)
	{
        var value = midtownSearchSugg.__AutoComplete[id]['element'].value;
		value=value.replace(/#/, "%23")
		if (value!="")
		{
      		midtownSearchSugg.get_suggestions("https://www.midtowncomics.com/store/rpc.asp?q=" + value,id);
		}
		else
		{
			midtownSearchSugg.hideDropdown("q");
		}
	}
	,get_suggestions:function(url,id){
			
			var sc=ServerConnectionFactory.getConnection();				
			sc.returnType=ServerConnection.RETURN_TEXT;
			sc.successCallback=function(_6){
					
												 midtownSearchSugg.load_suggestions(_6,id);
											   };
			sc.execute(url);
	}
	,load_suggestions:function(_7,id)
	{			
		
		var toDisplay = new Array();
        var newDiv    = null;
        var text      = null;
        var numItems  = midtownSearchSugg.__AutoComplete[id]['dropdown'].childNodes.length;

        // Remove all child nodes from dropdown
        while (midtownSearchSugg.__AutoComplete[id]['dropdown'].childNodes.length > 0) {
            midtownSearchSugg.__AutoComplete[id]['dropdown'].removeChild(midtownSearchSugg.__AutoComplete[id]['dropdown'].childNodes[0]);
        }
		
		if (_7!="")
		{
			midtownSearchSugg.__AutoComplete[id]['data']=eval("("+_7+")");			
			var objclass=midtownSearchSugg.__AutoComplete[id]['data'];
			//var strseries=""
			var data
			var i=0
			for( var sIndex in objclass ) {
				data=objclass[sIndex]
				
				newDiv = document.createElement('div');
				newDiv.className = 'autocomplete_item'; // Don't use setAttribute()
				newDiv.setAttribute('id', 'autocomplete_item_' + i);
				newDiv.setAttribute('index', i);
				newDiv.style.zIndex = '6010';
				
				newDiv.onmouseover = function() {
													midtownSearchSugg.AutoComplete_HighlightItem(midtownSearchSugg.__AutoComplete[id]['element'].getAttribute('id'), this.getAttribute('index'));
												};
           		newDiv.onclick     = function() {
													midtownSearchSugg.AutoComplete_SetValue(midtownSearchSugg.__AutoComplete[id]['element'].getAttribute('id')); 
													midtownSearchSugg.AutoComplete_HideDropdown(midtownSearchSugg.__AutoComplete[id]['element'].getAttribute('id'));
												
document.getElementById("frmsearchtop").submit();									
}
            
				
				text   = document.createTextNode(sIndex);
				newDiv.appendChild(text);
				
				midtownSearchSugg.__AutoComplete[id]['dropdown'].appendChild(newDiv);
				i=i+1;
			}
			// Show dropdown
			if (!midtownSearchSugg.__AutoComplete[id]['isVisible']) {
				midtownSearchSugg.__AutoComplete[id]['dropdown'].style.visibility = 'visible';
				midtownSearchSugg.__AutoComplete[id]['isVisible'] = true;
			}
			// If now showing less items than before, reset the highlighted value
			if (midtownSearchSugg.__AutoComplete[id]['dropdown'].childNodes.length != numItems) {
				midtownSearchSugg.__AutoComplete[id]['highlighted'] = null;
			}
			
		}
		else
		{
			//alert("hell0")
			midtownSearchSugg.__AutoComplete[id]['dropdown'].style.visibility = 'hidden';
			midtownSearchSugg.__AutoComplete[id]['isVisible'] = false;
			midtownSearchSugg.__AutoComplete[id]['highlighted'] = null;
		}
			
	}
	,AutoComplete_ScrollCheck:function(id)
	{
		 // Scroll down, or wrapping around from scroll up
        if (midtownSearchSugg.__AutoComplete[id]['highlighted'] > midtownSearchSugg.__AutoComplete[id]['lastItemShowing']) {
            midtownSearchSugg.__AutoComplete[id]['firstItemShowing'] = midtownSearchSugg.__AutoComplete[id]['highlighted'] - (midtownSearchSugg.__AutoComplete[id]['maxitems'] - 1);
            midtownSearchSugg.__AutoComplete[id]['lastItemShowing']  = midtownSearchSugg.__AutoComplete[id]['highlighted'];
        }
        
        // Scroll up, or wrapping around from scroll down
        if (midtownSearchSugg.__AutoComplete[id]['highlighted'] < midtownSearchSugg.__AutoComplete[id]['firstItemShowing']) {
            midtownSearchSugg.__AutoComplete[id]['firstItemShowing'] = midtownSearchSugg.__AutoComplete[id]['highlighted'];
            midtownSearchSugg.__AutoComplete[id]['lastItemShowing']  = midtownSearchSugg.__AutoComplete[id]['highlighted'] + (midtownSearchSugg.__AutoComplete[id]['maxitems'] - 1);
        }
        
        midtownSearchSugg.__AutoComplete[id]['dropdown'].scrollTop = midtownSearchSugg.__AutoComplete[id]['firstItemShowing'] * 15;
	}
	,AutoComplete_HideDropdown:function(id)
	{
		 if (midtownSearchSugg.__AutoComplete[id]['iframe']) {
            midtownSearchSugg.__AutoComplete[id]['iframe'].style.visibility = 'hidden';
        }
       midtownSearchSugg.__AutoComplete[id]['dropdown'].style.visibility = 'hidden';
       midtownSearchSugg.__AutoComplete[id]['highlighted'] = null;
       midtownSearchSugg.__AutoComplete[id]['isVisible']   = false;
	}
	,AutoComplete_Highlight:function(id, index)
	{
		// Out of bounds checking
		   		if (index == 1 && midtownSearchSugg.__AutoComplete[id]['highlighted'] == midtownSearchSugg.__AutoComplete[id]['dropdown'].childNodes.length - 1) {
					midtownSearchSugg.__AutoComplete[id]['dropdown'].childNodes[midtownSearchSugg.__AutoComplete[id]['highlighted']].className = 'autocomplete_item';
				   midtownSearchSugg.__AutoComplete[id]['highlighted'] = null;
				
				} else if (index == -1 && midtownSearchSugg.__AutoComplete[id]['highlighted'] == 0) {
					midtownSearchSugg.__AutoComplete[id]['dropdown'].childNodes[0].className = 'autocomplete_item';
					midtownSearchSugg.__AutoComplete[id]['highlighted'] = midtownSearchSugg.__AutoComplete[id]['dropdown'].childNodes.length;
				}
		
				// Nothing highlighted at the moment
				if (midtownSearchSugg.__AutoComplete[id]['highlighted'] == null) {
					midtownSearchSugg.__AutoComplete[id]['dropdown'].childNodes[0].className = 'autocomplete_item_highlighted';
					midtownSearchSugg.__AutoComplete[id]['highlighted'] = 0;
		
				} else {
					if (midtownSearchSugg.__AutoComplete[id]['dropdown'].childNodes[midtownSearchSugg.__AutoComplete[id]['highlighted']]) {
						midtownSearchSugg.__AutoComplete[id]['dropdown'].childNodes[midtownSearchSugg.__AutoComplete[id]['highlighted']].className = 'autocomplete_item';
					}
		
					var newIndex = parseInt(midtownSearchSugg.__AutoComplete[id]['highlighted']) + parseInt(index);
		
					if (midtownSearchSugg.__AutoComplete[id]['dropdown'].childNodes[newIndex]) {
						midtownSearchSugg.__AutoComplete[id]['dropdown'].childNodes[newIndex].className = 'autocomplete_item_highlighted';
						
						midtownSearchSugg.__AutoComplete[id]['highlighted'] = newIndex;
					}
				}
		
	}
	,AutoComplete_HighlightItem:function(id, index)
	{
		
		 if (midtownSearchSugg.__AutoComplete[id]['dropdown'].childNodes[index]) {
            for (var i=0; i<midtownSearchSugg.__AutoComplete[id]['dropdown'].childNodes.length; ++i) {
                if (midtownSearchSugg.__AutoComplete[id]['dropdown'].childNodes[i].className == 'autocomplete_item_highlighted') {
                    midtownSearchSugg.__AutoComplete[id]['dropdown'].childNodes[i].className = 'autocomplete_item';
                }
            }
            
            midtownSearchSugg.__AutoComplete[id]['dropdown'].childNodes[index].className = 'autocomplete_item_highlighted';
           midtownSearchSugg.__AutoComplete[id]['highlighted'] = index;
        }
	}
	,AutoComplete_SetValue:function(id)
	{
		midtownSearchSugg.__AutoComplete[id]['element'].value = midtownSearchSugg.__AutoComplete[id]['dropdown'].childNodes[midtownSearchSugg.__AutoComplete[id]['highlighted']].innerHTML;
	}
